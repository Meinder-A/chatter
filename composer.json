{
    "name": "meinder-a/forum",
    "description": "A simple forum package for your laravel app",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Tony Lea",
            "email": "tony@devdojo.com"
        },
        {
            "name": "Anthony Meinder",
            "email": "me@meinder-a.com"
        }
    ],
    "require": {
        "mews/purifier": "~3.0"
    },
    "require-dev": {
        "illuminate/cache": "^8.0",
        "illuminate/console": "^8.0",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
	    "psr-4": {
	      "MeinderA\\Forum\\": "/src"
		}
	},
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MeinderA\\Forum\\ForumServiceProvider"
            ]
        }
    }
}
